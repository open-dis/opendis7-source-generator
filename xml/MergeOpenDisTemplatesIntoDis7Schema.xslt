                <xsl:sort select="@href" order="ascending"/>

                <xsl:variable name="pduFamilyDocument" select="@href"/>
                <xsl:message>
                    <xsl:text>====================================================</xsl:text>
                </xsl:message>
                <xsl:message>
                    <xsl:text>xi:import href=</xsl:text>
                    <xsl:value-of select="$pduFamilyDocument"/>
