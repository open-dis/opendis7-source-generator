                    <xsl:value-of select="concat($relativePathToFiles,$pduFamilyDocument)"/>
                </xsl:message>

                <xsl:variable name="nodes" select="doc(concat($relativePathToFiles,$pduFamilyDocument))"/>
                <xsl:message>
