            <sisoenum type="RequiredReliabilityService" comment="uid 74"/>
        </attribute>
   
        <attribute name="pad1" comment="padding">
            <primitive type="uint8"/>
        </attribute>

        <attribute name="requestID" comment="Request ID">
            <primitive type="uint32"/>
        </attribute>
   
