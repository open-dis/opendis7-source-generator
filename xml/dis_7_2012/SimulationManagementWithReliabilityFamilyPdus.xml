    <class name="StopFreezeReliablePdu" inheritsFrom="SimulationManagementWithReliabilityFamilyPdu" comment="Section 5.3.12.4: Stop freeze simulation, relaible.">
      
        <initialValue name="pduType" value="DISPDUType.STOP_FREEZE_R"/>
   
